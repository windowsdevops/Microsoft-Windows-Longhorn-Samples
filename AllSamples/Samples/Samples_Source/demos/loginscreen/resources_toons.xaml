   <Style def:Name="CartoonText">

    <ContentPresenter  />

     <Style.VisualTree>
        <Text FontFamily="Comic Sans MS" Foreground="black" FontWeight="bold" FontSize="19pt" >
         <Text.TextContent>
			<Bind/>
