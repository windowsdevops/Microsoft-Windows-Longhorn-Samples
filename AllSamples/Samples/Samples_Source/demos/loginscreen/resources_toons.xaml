   <!-- This defines the style for other buttons to base off  *********************************-->
	<Style def:Name="ButtonStyler">
			 <Button Margin="0,0,20,20" Background="Gold"  />
			<Style.VisualTree>
			<TransformDecorator >
			    <Canvas Height="100%" Width="100%" Cursor="Hand" >
			        <Path def:StyleID="cartoonbuttonbg"  Stroke="Black" StrokeThickness="4" Fill="{cartoongradientdefault}" Data="M 0 42 L 182 0 L 161 78 C 161 78 16 80.9999923706055 16 81.9999923706055 C 16 82.9999923706055 0 42 0 42" />
			        <FlowPanel Width="100%" Height="100%" Margin="0,23,0,0">
			            <TransformDecorator  Transform="rotate -8">
				            <ContentPresenter  ContentControl.Content="*Alias(Target = Content)"
				            ContentControl.ContentStyle="{CartoonText}" />
			            </TransformDecorator>
			        </FlowPanel>
			    </Canvas>
			</TransformDecorator>
			</Style.VisualTree>
			<Style.VisualTriggers>
				<PropertyTrigger Property="IsMouseOver" Value="true">
					<Set PropertyPath="Shape.Fill" Value="{cartoongradienthover}" Target="cartoonbuttonbg"/>
				</PropertyTrigger>
			</Style.VisualTriggers>
		</Style>
		  <Style def:Name="LoginButton" BasedOn="{ButtonStyler}">
			 <Button Width="200" Height="100"  Margin="0" DockPanel.Dock="top"  />
			</Style>



		<Style def:Name="ManageUsersButton" BasedOn="{ButtonStyler}" >
			 <Button Visibility="collapsed" />
		</Style>
		<Style def:Name="GuestSigninButton" BasedOn="{ButtonStyler}">
			 <Button Visibility="collapsed" />
		</Style>
		<Style def:Name="ForgotPasswordButton" BasedOn="{ButtonStyler}">
			 <Button DockPanel.Dock="right" Visibility="collapsed" />
		</Style>


