<Project DefaultTargets="Build">
  <!-- MSBUILD Project File -->
	
  <PropertyGroup>

    <Property Version="1.0.0.0" />

    <Property SecurityManifest="Security.xml" />

    <Property Language="C#" />
		
    <Property DefaultClrNameSpace="SDKSample" />

    <Property TargetName="FullTrust_cs" />
			
    <Property HostInBrowser="False" />

  </PropertyGroup>
	
  <!--Import the target file that contains all the common targets -->
  <Import Project="$(LAPI)\WindowsApplication.target" />
		
  <ItemGroup>
    <!-- Application Definition Markup File -->
    <Item Type="ApplicationDefinition" Include="app.xaml" />

    <!-- Compiled Xaml Files -->
    <Item Type="Pages" Include="Default.xaml"/>
  </ItemGroup>

</Project>
