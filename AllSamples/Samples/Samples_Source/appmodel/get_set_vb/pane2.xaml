<!-- Typically, the root element will be one of the Panels: Canvas, DockPanel, FlowPanel, GridPanel, Table, TextPanel -->
<DockPanel  
    xmlns="http://schemas.microsoft.com/2003/xaml"
    xmlns:def="Definition"
    def:Class="Get_Set_VB.Pane2"
    def:CodeBehind="Pane2.xaml.vb" 
    Loaded="Init"
    >
    <Text Height="30"
			Margin="10,10,10,10"
            DockPanel.Dock="Top"
            ID="txtFromPage1">The Text From Page 1 is: </Text>
            
            
    <Button Click="btnClose"
            Height="30"
            Width="150"
			Margin="10,10,10,10"
            DockPanel.Dock="Top"
            Content="Close"/>
</DockPanel>